{
  "name": "abra-flexi",
  "version": "0.0.1",
  "description": "ABRA Flexi API Typescript client. Automaticly generated from API json annotations.",
  "types": "build/index.d.ts",
  "scripts": {
    "build:lib": "tsc --project tsconfig.src.json",
    "build:cli": "tsc --project tsconfig.generator.json && cp -r ./generator/templates bin/",
    "build": "tsc",
    "clean": "rimraf ./dist/* ./tsconfig.tsbuildInfo ./bin/*",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "dev": "npm run start:dev",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasKavan/abraFlexiClient.git"
  },
  "exports": {
    "import": "./build/src/index.js",
    "require": "./build/src/index.js"
  },
  "keywords": [
    "ABRA Flexi",
    "ABRA API",
    "ABRA Flexi API",
    "ABRA API Client"
  ],
  "author": "Tomas Kavan",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/tomasKavan/abraFlexiClient/issues"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/tomasKavan/abraFlexiClient#readme",
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.13.0",
    "@types/yargs": "^17.0.33",
    "ejs": "^3.1.10",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "big.js": "^6.2.2"
  },
  "publishConfig": {
    "directory": "build"
  }
}